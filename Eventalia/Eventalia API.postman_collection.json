										"exec": [
											"let locationHeader = pm.response.headers.get(\"Location\");\r",
											"\r",
											"if (locationHeader) {\r",
											"    let match = locationHeader.match(/\\/([^\\/]+)$/);\r",
											"    if(match){\r",
											"        pm.environment.set(\"idEvento\", match[1]);\r",
											"    }\r",
											"}"
										],
